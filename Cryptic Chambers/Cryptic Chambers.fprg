    <function name="printScoreboardToDisplay" type="None" variable="">
        <parameters>
            <parameter name="names" type="String" array="True"/>
            <parameter name="scores" type="Integer" array="True"/>
        </parameters>
        <body>
            <declare name="Display" type="String" array="True" size="size(names) + 8"/>
            <open expression="&quot;Scoreboard_Frame.txt&quot;" mode="read"/>
            <declare name="i" type="Integer" array="False" size=""/>
            <for variable="i" start="0" end="5" direction="inc" step="1">
                <read variable="Display[i]"/>
            </for>
            <for variable="i" start="size(Display) - 2" end="size(Display)-1" direction="inc" step="1">
                <read variable="Display[i]"/>
            </for>
            <close/>
            <declare name="place, offset, j" type="Integer" array="False" size=""/>
            <assign variable="offset" expression="6"/>
            <assign variable="place" expression="1"/>
            <declare name="winningChar, row" type="String" array="False" size=""/>
            <for variable="i" start="0" end="size(names)-1" direction="inc" step="1">
                <if expression="i &gt; 0">
                    <then>
                        <if expression="scores[i] &lt; scores[i-1]">
                            <then>
                                <assign variable="place" expression="place + 1"/>
                            </then>
                            <else/>
                        </if>
                    </then>
                    <else/>
                </if>
                <assign variable="winningChar" expression="ToString(place)"/>
                <assign variable="row" expression="&quot;##   &quot; &amp; winningChar &amp; &quot;.&quot;"/>
                <for variable="j" start="0" end="3 - len(winningChar) - 1" direction="inc" step="1">
                    <assign variable="row" expression="row &amp; &quot; &quot;"/>
                </for>
                <assign variable="row" expression="row &amp; names[i]"/>
                <for variable="j" start="0" end="25 - len(names[i]) - 1" direction="inc" step="1">
                    <assign variable="row" expression="row &amp; &quot;.&quot;"/>
                </for>
                <assign variable="row" expression="row &amp; ToString(scores[i])"/>
                <for variable="j" start="0" end="8 - len(ToString(scores[i])) - 1" direction="inc" step="1">
                    <assign variable="row" expression="row &amp; &quot; &quot;"/>
                </for>
                <assign variable="row" expression="row &amp; &quot;##&quot;"/>
                <assign variable="display[i+offset]" expression="row"/>
            </for>
            <open expression="&quot;Cryptic Chambers Display.txt&quot;" mode="write"/>
            <for variable="i" start="0" end="size(Display)-1" direction="inc" step="1">
                <write expression="Display[i]"/>
            </for>
            <close/>
            <output expression="&quot;Hit ENTER to proceed&quot;" newline="True"/>
            <declare name="proceed" type="String" array="False" size=""/>
            <input variable="proceed"/>
        </body>
    </function>
