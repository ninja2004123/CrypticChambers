    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="Variable Declaration"/>
            <declare name="userName" type="String" array="False" size=""/>
            <declare name="shouldQuit" type="Boolean" array="False" size=""/>
            <declare name="score" type="Integer" array="False" size=""/>
            <comment text="Engage user in story"/>
            <call expression="playIntroOutro(&quot;Intro&quot;)"/>
            <assign variable="userName" expression="promptUserForName()"/>
            <assign variable="shouldQuit" expression="false"/>
            <while expression="!shouldQuit">
                <declare name="gameState" type="String" array="False" size=""/>
                <assign variable="gameState" expression="chooseGameState()"/>
                <if expression="gameState == &quot;START&quot;">
                    <then>
                        <assign variable="score" expression="levelMaze()"/>
                        <call expression="updateUserScore(userName, score)"/>
                    </then>
                    <else/>
                </if>
                <if expression="gameState == &quot;SCOREBOARD&quot;">
                    <then>
                        <call expression="displayScoreboard()"/>
                    </then>
                    <else/>
                </if>
                <if expression="gameState == &quot;QUIT&quot;">
                    <then>
                        <assign variable="shouldQuit" expression="true"/>
                        <call expression="playEnding(&quot;Farewell&quot;)"/>
                        <call expression="playEnding(&quot;Quit&quot;)"/>
                    </then>
                    <else/>
                </if>
            </while>
        </body>
    </function>
    <function name="chooseGameState" type="String" variable="gameState">
        <parameters/>
        <body>
            <declare name="gameState, userInput" type="String" array="False" size=""/>
            <declare name="currentGameStateIndex" type="Integer" array="False" size=""/>
            <assign variable="currentGameStateIndex" expression="0"/>
            <do expression="userInput != &quot;&quot;">
                <if expression="currentGameStateIndex &gt;= 0">
                    <then>
                        <assign variable="currentGameStateIndex" expression="currentGameStateIndex % 3"/>
                    </then>
                    <else>
                        <assign variable="currentGameStateIndex" expression="3 - abs(currentGameStateIndex) % 3"/>
                    </else>
                </if>
                <call expression="printGameMenu(currentGameStateIndex)"/>
                <output expression="&quot;Use W/w and S/s to select an option (after choosing click ENTER to lock in selection)&quot;" newline="True"/>
                <input variable="userInput"/>
                <if expression="userInput == &quot;W&quot; || userInput == &quot;w&quot;">
                    <then>
                        <assign variable="currentGameStateIndex" expression="currentGameStateIndex - 1"/>
                    </then>
                    <else/>
                </if>
                <if expression="userInput == &quot;S&quot; || userInput == &quot;s&quot;">
                    <then>
                        <assign variable="currentGameStateIndex" expression="currentGameStateIndex + 1"/>
                    </then>
                    <else/>
                </if>
            </do>
            <assign variable="gameState" expression="getCurrentGameState(currentGameStateIndex)"/>
        </body>
    </function>
    <function name="displayScoreboard" type="None" variable="">
        <parameters/>
        <body>
            <declare name="numberOfPlayers" type="Integer" array="False" size=""/>
            <assign variable="numberOfPlayers" expression="getLengthOfFile(&quot;Scoreboard.txt&quot;)"/>
            <if expression="numberOfPlayers &gt; 0">
                <then>
                    <declare name="usernames" type="String" array="True" size="numberOfPlayers"/>
                    <declare name="scores" type="Integer" array="True" size="numberOfPlayers"/>
                    <call expression="getUsernamesAndScores(usernames, scores)"/>
                    <call expression="mergeSort(usernames, scores, 0, size(usernames)-1)"/>
                    <call expression="printScoreboardToDisplay(usernames, scores)"/>
                </then>
                <else>
                    <declare name="usernames" type="String" array="True" size="1"/>
                    <declare name="scores" type="Integer" array="True" size="1"/>
                    <assign variable="usernames[0]" expression="&quot;Gilbert&quot;"/>
                    <assign variable="scores[0]" expression="10000"/>
                    <call expression="printScoreboardToDisplay(usernames, scores)"/>
                </else>
            </if>
        </body>
    </function>
    <function name="drawTicTacToeBoard" type="None" variable="">
        <parameters>
            <parameter name="board" type="String" array="True"/>
        </parameters>
        <body>
            <declare name="tictactoeFileGrid" type="String" array="True" size="30"/>
            <call expression="readFileToArray(&quot;TicTacToe_Frame.txt&quot;, tictactoeFileGrid)"/>
            <declare name="XDrawings, ODrawings, Sign" type="String" array="True" size="9"/>
            <call expression="readFileToArray(&quot;TicTacToe_X_Character.txt&quot;, XDrawings)"/>
            <call expression="readFileToArray(&quot;TicTacToe_O_Character.txt&quot;, ODrawings)"/>
            <declare name="box, row, col" type="Integer" array="False" size=""/>
            <for variable="box" start="0" end="8" direction="inc" step="1">
                <assign variable="row" expression="box / 3"/>
                <assign variable="col" expression="box % 3"/>
                <declare name="i" type="Integer" array="False" size=""/>
                <if expression="char(Board[row], col) == &quot;X&quot;">
                    <then>
                        <for variable="i" start="0" end="size(Sign) - 1" direction="inc" step="1">
                            <assign variable="Sign[i]" expression="XDrawings[i]"/>
                        </for>
                    </then>
                    <else>
                        <if expression="char(Board[row], col) == &quot;O&quot;">
                            <then>
                                <for variable="i" start="0" end="size(Sign) - 1" direction="inc" step="1">
                                    <assign variable="Sign[i]" expression="ODrawings[i]"/>
                                </for>
                            </then>
                            <else/>
                        </if>
                    </else>
                </if>
                <if expression="char(Board[row], col) != &quot; &quot;">
                    <then>
                        <declare name="rowToEdit, colToEdit, j, k" type="Integer" array="False" size=""/>
                        <declare name="newRow" type="String" array="False" size=""/>
                        <assign variable="rowToEdit" expression="row * 10 + 1"/>
                        <for variable="i" start="0" end="size(Sign) - 1" direction="inc" step="1">
                            <assign variable="k" expression="0"/>
                            <assign variable="newRow" expression="&quot;&quot;"/>
                            <assign variable="colToEdit" expression="col * 14 + 2"/>
                            <for variable="j" start="0" end="len(tictactoeFileGrid[rowToEdit]) - 1" direction="inc" step="1">
                                <if expression="k &lt; len(Sign[i]) &amp;&amp; j == colToEdit">
                                    <then>
                                        <if expression="char(Sign[i], k) == &quot;%&quot;">
                                            <then>
                                                <assign variable="newRow" expression="newRow &amp; &quot; &quot;"/>
                                            </then>
                                            <else>
                                                <assign variable="newRow" expression="newRow &amp; char(Sign[i], k)"/>
                                            </else>
                                        </if>
                                        <assign variable="k" expression="k + 1"/>
                                        <assign variable="colToEdit" expression="colToEdit + 1"/>
                                    </then>
                                    <else>
                                        <assign variable="newRow" expression="newRow &amp; char(tictactoeFileGrid[rowToEdit], j)"/>
                                    </else>
                                </if>
                            </for>
                            <assign variable="tictactoeFileGrid[rowToEdit]" expression="newRow"/>
                            <assign variable="rowToEdit" expression="rowToEdit + 1"/>
                        </for>
                    </then>
                    <else/>
                </if>
            </for>
            <call expression="printArrayToDisplay(tictactoeFileGrid)"/>
        </body>
    </function>
    <function name="gameOver" type="Integer" variable="gameOutcome">
        <parameters>
            <parameter name="board" type="String" array="True"/>
        </parameters>
        <body>
            <declare name="stateVictoryForX, stateVictoryForO, stateDraw, stateGameNotOver, gameNotOver" type="Integer" array="False" size=""/>
            <assign variable="gameNotOver" expression="0"/>
            <assign variable="stateVictoryForX" expression="1"/>
            <assign variable="stateDraw" expression="2"/>
            <assign variable="stateVictoryForO" expression="3"/>
            <declare name="gameOutcome" type="Integer" array="False" size=""/>
            <declare name="i" type="Integer" array="False" size=""/>
            <assign variable="gameOutcome" expression="-1"/>
            <for variable="i" start="0" end="2" direction="inc" step="1">
                <if expression="char(board[i], 0) == char(board[i], 1) &amp;&amp;&#13;&#10;char(board[i], 1) == char(board[i], 2)">
                    <then>
                        <if expression="char(board[i], 1) == &quot;X&quot;">
                            <then>
                                <assign variable="gameOutcome" expression="stateVictoryForX"/>
                            </then>
                            <else>
                                <if expression="char(board[i], 1) == &quot;O&quot;">
                                    <then>
                                        <assign variable="gameOutcome" expression="stateVictoryForO"/>
                                    </then>
                                    <else/>
                                </if>
                            </else>
                        </if>
                    </then>
                    <else/>
                </if>
                <if expression="char(board[0], i) == char(board[1], i) &amp;&amp;&#13;&#10;char(board[1], i) == char(board[2], i)">
                    <then>
                        <if expression="char(board[1], i) == &quot;X&quot;">
                            <then>
                                <assign variable="gameOutcome" expression="stateVictoryForX"/>
                            </then>
                            <else>
                                <if expression="char(board[1], i) == &quot;O&quot;">
                                    <then>
                                        <assign variable="gameOutcome" expression="stateVictoryForO"/>
                                    </then>
                                    <else/>
                                </if>
                            </else>
                        </if>
                    </then>
                    <else/>
                </if>
            </for>
            <if expression="char(board[0], 0) == char(board[1], 1) &amp;&amp;&#13;&#10;char(board[1], 1) == char(board[2], 2)">
                <then>
                    <if expression="char(board[1], 1) == &quot;X&quot;">
                        <then>
                            <assign variable="gameOutcome" expression="stateVictoryForX"/>
                        </then>
                        <else>
                            <if expression="char(board[1], 1) == &quot;O&quot;">
                                <then>
                                    <assign variable="gameOutcome" expression="stateVictoryForO"/>
                                </then>
                                <else/>
                            </if>
                        </else>
                    </if>
                </then>
                <else/>
            </if>
            <if expression="char(board[0], 2) == char(board[1], 1) &amp;&amp;&#13;&#10;char(board[1], 1) == char(board[2], 0)">
                <then>
                    <if expression="char(board[1], 1) == &quot;X&quot;">
                        <then>
                            <assign variable="gameOutcome" expression="stateVictoryForX"/>
                        </then>
                        <else>
                            <if expression="char(board[1], 1) == &quot;O&quot;">
                                <then>
                                    <assign variable="gameOutcome" expression="stateVictoryForO"/>
                                </then>
                                <else/>
                            </if>
                        </else>
                    </if>
                </then>
                <else/>
            </if>
            <if expression="gameOutcome == -1">
                <then>
                    <assign variable="gameOutcome" expression="stateDraw"/>
                    <declare name="move" type="Integer" array="False" size=""/>
                    <for variable="move" start="0" end="8" direction="inc" step="1">
                        <if expression="isTTTMoveValid(move, board)">
                            <then>
                                <assign variable="gameOutcome" expression="gameNotOver"/>
                            </then>
                            <else/>
                        </if>
                    </for>
                </then>
                <else/>
            </if>
        </body>
    </function>
    <function name="getCurrentGameState" type="String" variable="gameState">
        <parameters>
            <parameter name="index" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="gameState" type="String" array="False" size=""/>
            <comment text="Set-up game states array"/>
            <declare name="numOfGameStates" type="Integer" array="False" size=""/>
            <assign variable="numOfGameStates" expression="3"/>
            <declare name="gameStates" type="String" array="True" size="numOfGameStates"/>
            <assign variable="gameStates[0]" expression="&quot;START&quot;"/>
            <assign variable="gameStates[1]" expression="&quot;SCOREBOARD&quot;"/>
            <assign variable="gameStates[2]" expression="&quot;QUIT&quot;"/>
            <comment text="Make sure index is in range"/>
            <if expression="index &gt;= 0">
                <then>
                    <assign variable="index" expression="index % numOfGameStates"/>
                </then>
                <else>
                    <assign variable="index" expression="numOfGameStates - (abs(index) % numOfGameStates)"/>
                </else>
            </if>
            <assign variable="gameState" expression="gameStates[index]"/>
        </body>
    </function>
    <function name="getLengthOfFile" type="Integer" variable="lengthOfFile">
        <parameters>
            <parameter name="nameOfFile" type="String" array="False"/>
        </parameters>
        <body>
            <declare name="trash" type="String" array="False" size=""/>
            <declare name="lengthOfFile" type="Integer" array="False" size=""/>
            <open expression="nameOfFile" mode="read"/>
            <assign variable="lengthOfFile" expression="0"/>
            <while expression="!EOF()">
                <assign variable="lengthOfFile" expression="lengthOfFile + 1"/>
                <read variable="trash"/>
            </while>
            <close/>
        </body>
    </function>
    <function name="GetStringFromFile" type="String" variable="stringFromFile">
        <parameters>
            <parameter name="fileName" type="String" array="False"/>
        </parameters>
        <body>
            <declare name="stringFromFile" type="String" array="False" size=""/>
            <open expression="fileName" mode="read"/>
            <assign variable="stringFromFile" expression="&quot;&quot;"/>
            <while expression="!EOF()">
                <if expression="len(stringFromFile) &gt; 0">
                    <then>
                        <assign variable="stringFromFile" expression="stringFromFile &amp; ToChar(10)"/>
                    </then>
                    <else/>
                </if>
                <declare name="tmp" type="String" array="False" size=""/>
                <read variable="tmp"/>
                <assign variable="stringFromFile" expression="stringFromFile &amp; tmp"/>
            </while>
            <close/>
        </body>
    </function>
    <function name="getUsernamesAndScores" type="None" variable="">
        <parameters>
            <parameter name="usernames" type="String" array="True"/>
            <parameter name="scores" type="Integer" array="True"/>
        </parameters>
        <body>
            <declare name="line, name, score" type="String" array="False" size=""/>
            <declare name="i, lineNumber" type="Integer" array="False" size=""/>
            <open expression="&quot;scoreboard.txt&quot;" mode="read"/>
            <assign variable="lineNumber" expression="0"/>
            <while expression="!EOF()">
                <assign variable="name" expression="&quot;&quot;"/>
                <assign variable="score" expression="&quot;&quot;"/>
                <read variable="line"/>
                <assign variable="i" expression="0"/>
                <while expression="char(line, i) != &quot;,&quot;">
                    <assign variable="name" expression="name &amp; char(line, i)"/>
                    <assign variable="i" expression="i + 1"/>
                </while>
                <assign variable="i" expression="i + 1"/>
                <while expression="i &lt; len(line)">
                    <assign variable="score" expression="score &amp; char(line, i)"/>
                    <assign variable="i" expression="i + 1"/>
                </while>
                <assign variable="usernames[lineNumber]" expression="name"/>
                <assign variable="scores[lineNumber]" expression="ToInteger(score)"/>
                <assign variable="lineNumber" expression="lineNumber + 1"/>
            </while>
            <close/>
        </body>
    </function>
    <function name="isMoveValid" type="Boolean" variable="moveIsValid">
        <parameters>
            <parameter name="Map" type="String" array="True"/>
            <parameter name="PlayerXY" type="Integer" array="True"/>
        </parameters>
        <body>
            <declare name="moveIsValid" type="Boolean" array="False" size=""/>
            <if expression="PlayerXY[0] &lt; 0 || PlayerXY[0] &gt;= 29">
                <then>
                    <assign variable="moveIsValid" expression="true"/>
                </then>
                <else>
                    <if expression="char(Map[PlayerXY[0]], PlayerXY[1]) == &quot; &quot;">
                        <then>
                            <assign variable="moveIsValid" expression="true"/>
                        </then>
                        <else>
                            <assign variable="moveIsValid" expression="false"/>
                        </else>
                    </if>
                </else>
            </if>
        </body>
    </function>
    <function name="isTowerOfHanoiInputValid" type="Boolean" variable="isValid">
        <parameters>
            <parameter name="input" type="String" array="False"/>
            <parameter name="adminCode" type="String" array="False"/>
        </parameters>
        <body>
            <declare name="isValid" type="Boolean" array="False" size=""/>
            <if expression="input == adminCode || input == &quot;QUIT&quot;">
                <then>
                    <assign variable="isValid" expression="True"/>
                </then>
                <else>
                    <if expression="len(input) != 2">
                        <then>
                            <assign variable="isValid" expression="False"/>
                        </then>
                        <else>
                            <if expression="char(input, 0) != &quot;1&quot; &amp;&amp; &#13;&#10;char(input, 0) != &quot;2&quot; &amp;&amp;&#13;&#10;char(input, 0) != &quot;3&quot;">
                                <then>
                                    <assign variable="isValid" expression="False"/>
                                </then>
                                <else>
                                    <if expression="char(input, 1) != &quot;1&quot; &amp;&amp; &#13;&#10;char(input, 1) != &quot;2&quot; &amp;&amp;&#13;&#10;char(input, 1) != &quot;3&quot;">
                                        <then>
                                            <assign variable="isValid" expression="False"/>
                                        </then>
                                        <else>
                                            <if expression="char(input, 0) == char(input, 1)">
                                                <then>
                                                    <assign variable="isValid" expression="False"/>
                                                </then>
                                                <else>
                                                    <assign variable="isValid" expression="True"/>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
        </body>
    </function>
    <function name="isTTTMoveValid" type="Boolean" variable="moveIsValid">
        <parameters>
            <parameter name="move" type="Integer" array="False"/>
            <parameter name="board" type="String" array="True"/>
        </parameters>
        <body>
            <declare name="moveIsValid" type="Boolean" array="False" size=""/>
            <assign variable="moveIsValid" expression="False"/>
            <if expression="move &gt;= 0 &amp;&amp; move &lt;= 8">
                <then>
                    <declare name="row, col" type="Integer" array="False" size=""/>
                    <assign variable="row" expression="move / 3"/>
                    <assign variable="col" expression="move % 3"/>
                    <if expression="char(board[row], col) == &quot; &quot;">
                        <then>
                            <assign variable="moveIsValid" expression="True"/>
                        </then>
                        <else/>
                    </if>
                </then>
                <else>
                    <if expression="move == 31336">
                        <then>
                            <assign variable="moveIsValid" expression="True"/>
                        </then>
                        <else/>
                    </if>
                </else>
            </if>
        </body>
    </function>
    <function name="levelHanoi" type="Integer" variable="score">
        <parameters>
            <parameter name="score" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="LoadingScreen" type="String" array="True" size="29"/>
            <call expression="readFileToArray(&quot;Hanoi_Loading_Screen.txt&quot;, LoadingScreen)"/>
            <call expression="printArrayToDisplay(LoadingScreen)"/>
            <declare name="shouldProceed" type="String" array="False" size=""/>
            <output expression="&quot;Type 'yes' if you wish to proceed to the next level. Be careful not to lose everything.&quot;" newline="True"/>
            <input variable="shouldProceed"/>
            <if expression="makeWordLowercase(shouldProceed) == &quot;yes&quot;">
                <then>
                    <declare name="first, second, third" type="String" array="True" size="6"/>
                    <declare name="i" type="Integer" array="False" size=""/>
                    <for variable="i" start="0" end="5" direction="inc" step="1">
                        <assign variable="first[i]" expression="&quot;#&quot;"/>
                        <assign variable="second[i]" expression="&quot;#&quot;"/>
                        <assign variable="third[i]" expression="&quot;#&quot;"/>
                    </for>
                    <assign variable="first[2]" expression="&quot;(O)&quot;"/>
                    <assign variable="first[3]" expression="&quot;(OOO)&quot;"/>
                    <assign variable="first[4]" expression="&quot;(OOOOO)&quot;"/>
                    <assign variable="first[5]" expression="&quot;(OOOOOOO)&quot;"/>
                    <call expression="printTowerOfHanoi(first, second, third)"/>
                    <declare name="getFromIndex, putToIndex" type="Integer" array="False" size=""/>
                    <assign variable="getFromIndex" expression="0"/>
                    <assign variable="putToIndex" expression="0"/>
                    <declare name="adminCode" type="String" array="False" size=""/>
                    <assign variable="adminCode" expression="&quot;Hearing that favorite song on repeat&quot;"/>
                    <declare name="userMadeMove" type="String" array="False" size=""/>
                    <assign variable="userMadeMove" expression="&quot;&quot;"/>
                    <while expression="!(third[2] == &quot;(O)&quot;) &amp;&amp; !(userMadeMove == adminCode) &amp;&amp; !(userMadeMove == &quot;QUIT&quot;)">
                        <do expression="!isTowerOfHanoiInputValid(userMadeMove, adminCode)">
                            <output expression="&quot;Make your move. ('12' to move from tower one to tower two)&quot;" newline="True"/>
                            <input variable="userMadeMove"/>
                        </do>
                        <if expression="userMadeMove == adminCode || userMadeMove == &quot;QUIT&quot;">
                            <then>
                                <assign variable="getFromIndex" expression="0"/>
                                <assign variable="putToIndex" expression="0"/>
                            </then>
                            <else>
                                <assign variable="getFromIndex" expression="ToInteger(char(userMadeMove, 0))"/>
                                <assign variable="putToIndex" expression="ToInteger(char(userMadeMove, 1))"/>
                            </else>
                        </if>
                        <declare name="poleToTakeFrom, poleToPutTo" type="String" array="True" size="6"/>
                        <if expression="getFromIndex == 1">
                            <then>
                                <for variable="i" start="0" end="5" direction="inc" step="1">
                                    <assign variable="poleToTakeFrom[i]" expression="first[i]"/>
                                </for>
                            </then>
                            <else>
                                <if expression="getFromIndex == 2">
                                    <then>
                                        <for variable="i" start="0" end="5" direction="inc" step="1">
                                            <assign variable="poleToTakeFrom[i]" expression="second[i]"/>
                                        </for>
                                    </then>
                                    <else>
                                        <for variable="i" start="0" end="5" direction="inc" step="1">
                                            <assign variable="poleToTakeFrom[i]" expression="third[i]"/>
                                        </for>
                                    </else>
                                </if>
                            </else>
                        </if>
                        <if expression="putToIndex == 1">
                            <then>
                                <for variable="i" start="0" end="5" direction="inc" step="1">
                                    <assign variable="poleToPutTo[i]" expression="first[i]"/>
                                </for>
                            </then>
                            <else>
                                <if expression="putToIndex == 2">
                                    <then>
                                        <for variable="i" start="0" end="5" direction="inc" step="1">
                                            <assign variable="poleToPutTo[i]" expression="second[i]"/>
                                        </for>
                                    </then>
                                    <else>
                                        <for variable="i" start="0" end="5" direction="inc" step="1">
                                            <assign variable="poleToPutTo[i]" expression="third[i]"/>
                                        </for>
                                    </else>
                                </if>
                            </else>
                        </if>
                        <declare name="fromPole, toPole" type="Integer" array="False" size=""/>
                        <assign variable="fromPole" expression="0"/>
                        <declare name="found" type="Boolean" array="False" size=""/>
                        <assign variable="found" expression="false"/>
                        <while expression="fromPole &lt; size(poleToTakeFrom) &amp;&amp; !found">
                            <if expression="poleToTakeFrom[fromPole] == &quot;#&quot;">
                                <then>
                                    <assign variable="fromPole" expression="fromPole + 1"/>
                                </then>
                                <else>
                                    <assign variable="found" expression="true"/>
                                </else>
                            </if>
                        </while>
                        <if expression="fromPole &gt;= size(poleToTakeFrom)">
                            <then>
                                <assign variable="fromPole" expression="size(poleToTakeFrom)-1"/>
                            </then>
                            <else/>
                        </if>
                        <declare name="shouldMovePole" type="Boolean" array="False" size=""/>
                        <assign variable="shouldMovePole" expression="true"/>
                        <assign variable="toPole" expression="size(poleToPutTo)-1"/>
                        <assign variable="found" expression="false"/>
                        <while expression="toPole &gt;= 0 &amp;&amp; !found">
                            <if expression="(poleToPutTo[topole] != &quot;#&quot;) &amp;&amp; len(poleToTakeFrom[fromPole]) &gt; len(poleToPutTo[topole])">
                                <then>
                                    <output expression="&quot;You cannot move a larger pole onto a smaller one.&quot;" newline="True"/>
                                    <assign variable="shouldMovePole" expression="False"/>
                                    <assign variable="found" expression="true"/>
                                </then>
                                <else/>
                            </if>
                            <if expression="poleToPutTo[toPole] != &quot;#&quot;">
                                <then>
                                    <assign variable="toPole" expression="toPole - 1"/>
                                </then>
                                <else>
                                    <assign variable="found" expression="true"/>
                                </else>
                            </if>
                        </while>
                        <if expression="shouldMovePole">
                            <then>
                                <declare name="Tmp" type="String" array="False" size=""/>
                                <assign variable="Tmp" expression="poleToTakeFrom[fromPole]"/>
                                <assign variable="poleToTakeFrom[fromPole]" expression="poleToPutTo[toPole]"/>
                                <assign variable="poleToPutTo[toPole]" expression="Tmp"/>
                            </then>
                            <else/>
                        </if>
                        <if expression="getFromIndex == 1">
                            <then>
                                <for variable="i" start="0" end="5" direction="inc" step="1">
                                    <assign variable="first[i]" expression="poleToTakeFrom[i]"/>
                                </for>
                            </then>
                            <else>
                                <if expression="getFromIndex == 2">
                                    <then>
                                        <for variable="i" start="0" end="5" direction="inc" step="1">
                                            <assign variable="second[i]" expression="poleToTakeFrom[i]"/>
                                        </for>
                                    </then>
                                    <else>
                                        <for variable="i" start="0" end="5" direction="inc" step="1">
                                            <assign variable="third[i]" expression="poleToTakeFrom[i]"/>
                                        </for>
                                    </else>
                                </if>
                            </else>
                        </if>
                        <if expression="putToIndex == 1">
                            <then>
                                <for variable="i" start="0" end="5" direction="inc" step="1">
                                    <assign variable="first[i]" expression="poleToPutTo[i]"/>
                                </for>
                            </then>
                            <else>
                                <if expression="putToIndex == 2">
                                    <then>
                                        <for variable="i" start="0" end="5" direction="inc" step="1">
                                            <assign variable="second[i]" expression="poleToPutTo[i]"/>
                                        </for>
                                    </then>
                                    <else>
                                        <for variable="i" start="0" end="5" direction="inc" step="1">
                                            <assign variable="third[i]" expression="poleToPutTo[i]"/>
                                        </for>
                                    </else>
                                </if>
                            </else>
                        </if>
                        <call expression="printTowerOfHanoi(first, second, third)"/>
                    </while>
                    <if expression="userMadeMove != &quot;QUIT&quot;">
                        <then>
                            <call expression="playEnding(&quot;Level Passed&quot;)"/>
                            <assign variable="score" expression="levelWordsearch(score + 1000)"/>
                        </then>
                        <else>
                            <assign variable="score" expression="0"/>
                            <call expression="playEnding(&quot;Defeat&quot;)"/>
                        </else>
                    </if>
                </then>
                <else/>
            </if>
        </body>
    </function>
    <function name="levelMaze" type="Integer" variable="score">
        <parameters/>
        <body>
            <declare name="move" type="String" array="False" size=""/>
