    <function name="robotMakesMove" type="None" variable="">
        <parameters>
            <parameter name="board" type="String" array="True"/>
        </parameters>
        <body>
            <declare name="bestScore, bestMove, score, move" type="Integer" array="False" size=""/>
            <declare name="numberOfMadeMoves" type="Integer" array="False" size=""/>
            <assign variable="numberOfMadeMoves" expression="0"/>
            <for variable="move" start="0" end="8" direction="inc" step="1">
                <if expression="!isTTTMoveValid(move, board)">
                    <then>
                        <assign variable="numberOfMadeMoves" expression="numberOfMadeMoves + 1"/>
                    </then>
                    <else/>
                </if>
            </for>
            <comment text="It takes too long to calculate every position from move 0, start calculating from second move and place the first move randomly. This also gives a chance for the user to win."/>
            <if expression="numberOfMadeMoves &gt; 1">
                <then>
                    <assign variable="bestScore" expression="0"/>
                    <assign variable="bestMove" expression="0"/>
                    <for variable="move" start="0" end="8" direction="inc" step="1">
                        <if expression="isTTTMoveValid(move, board)">
                            <then>
                                <call expression="setChar(board, move, &quot;O&quot;)"/>
                                <assign variable="score" expression="minimax(board, False)"/>
                                <call expression="setChar(board, move, &quot; &quot;)"/>
                                <if expression="score &gt; bestScore">
                                    <then>
                                        <assign variable="bestScore" expression="score"/>
                                        <assign variable="bestMove" expression="move"/>
                                    </then>
                                    <else/>
                                </if>
                            </then>
                            <else/>
                        </if>
                    </for>
                </then>
                <else>
                    <do expression="!(isTTTMoveValid(bestMove, board))">
                        <assign variable="bestMove" expression="random(9)"/>
                    </do>
                </else>
            </if>
            <call expression="setChar(board, bestMove, &quot;O&quot;)"/>
        </body>
    </function>
    <function name="setChar" type="None" variable="">
        <parameters>
            <parameter name="board" type="String" array="True"/>
            <parameter name="move" type="Integer" array="False"/>
            <parameter name="charToSet" type="String" array="False"/>
        </parameters>
        <body>
            <declare name="row, col, i" type="Integer" array="False" size=""/>
            <assign variable="row" expression="move / 3"/>
            <assign variable="col" expression="move % 3"/>
            <declare name="newString" type="String" array="False" size=""/>
            <assign variable="newString" expression="&quot;&quot;"/>
            <for variable="i" start="0" end="len(board[row])-1" direction="inc" step="1">
                <if expression="i == col">
                    <then>
                        <assign variable="newString" expression="newString &amp; charToSet"/>
                    </then>
                    <else>
                        <assign variable="newString" expression="newString &amp; char(board[row], i)"/>
                    </else>
                </if>
            </for>
            <assign variable="board[row]" expression="newString"/>
        </body>
    </function>
