    <function name="printTowerOfHanoi" type="None" variable="">
        <parameters>
            <parameter name="first" type="String" array="True"/>
            <parameter name="second" type="String" array="True"/>
            <parameter name="third" type="String" array="True"/>
        </parameters>
        <body>
            <declare name="zeroToOneSpacing, oneToTwoSpacing, twoToThreeSpacing" type="Integer" array="False" size=""/>
            <assign variable="zeroToOneSpacing" expression="11"/>
            <assign variable="oneToTwoSpacing" expression="22"/>
            <assign variable="twoToThreeSpacing" expression="22"/>
            <declare name="HanoiGameArray" type="String" array="True" size="13"/>
            <declare name="i, j" type="Integer" array="False" size=""/>
            <open expression="&quot;Hanoi_Frame.txt&quot;" mode="read"/>
            <for variable="i" start="0" end="4" direction="inc" step="1">
                <read variable="HanoiGameArray[i]"/>
            </for>
            <for variable="i" start="11" end="12" direction="inc" step="1">
                <read variable="HanoiGameArray[i]"/>
            </for>
            <close/>
            <for variable="i" start="0" end="size(first)-1" direction="inc" step="1">
                <declare name="Row" type="String" array="False" size=""/>
                <assign variable="Row" expression="&quot;#&quot;"/>
                <for variable="j" start="0" end="zeroToOneSpacing - (len(first[i]) - 1) / 2 - 1 " direction="inc" step="1">
                    <assign variable="Row" expression="Row &amp; &quot; &quot;"/>
                </for>
                <assign variable="Row" expression="Row &amp; first[i]"/>
                <for variable="j" start="0" end="oneToTwoSpacing - (len(second[i]) - 1) / 2 - (len(first[i]) - 1) / 2 - 1" direction="inc" step="1">
                    <assign variable="Row" expression="Row &amp; &quot; &quot;"/>
                </for>
                <assign variable="Row" expression="Row &amp; second[i]"/>
                <for variable="j" start="0" end="twoToThreeSpacing - ((len(third[i]) - 1) / 2) - ((len(second[i]) - 1) / 2) - 1" direction="inc" step="1">
                    <assign variable="Row" expression="Row &amp; &quot; &quot;"/>
                </for>
                <assign variable="Row" expression="Row &amp; third[i]"/>
                <for variable="j" start="0" end="zeroToOneSpacing - (len(third[i]) - 1) / 2 - 1 " direction="inc" step="1">
                    <assign variable="Row" expression="Row &amp; &quot; &quot;"/>
                </for>
                <assign variable="Row" expression="Row &amp; &quot;#&quot;"/>
                <assign variable="HanoiGameArray[i + 5]" expression="Row"/>
            </for>
            <call expression="printArrayToDisplay(HanoiGameArray)"/>
        </body>
    </function>
