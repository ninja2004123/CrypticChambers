    <function name="stringOneIsAlphabeticallyBiggerThanStringTwo" type="Boolean" variable="isBigger">
        <parameters>
            <parameter name="str1" type="String" array="False"/>
            <parameter name="str2" type="String" array="False"/>
        </parameters>
        <body>
            <declare name="isBigger, found" type="Boolean" array="False" size=""/>
            <declare name="minSize, i" type="Integer" array="False" size=""/>
            <assign variable="minSize" expression="len(str1)"/>
            <assign variable="isBigger" expression="False"/>
            <if expression="len(str2) &lt; minSize">
                <then>
                    <assign variable="minSize" expression="len(str2)"/>
                </then>
                <else/>
            </if>
            <assign variable="i" expression="0"/>
            <assign variable="found" expression="False"/>
            <while expression="i &lt; minSize &amp;&amp; !found">
                <if expression="ToCode(Char(str1, i)) &gt; ToCode(Char(str2, i))">
                    <then>
                        <assign variable="found" expression="True"/>
                        <assign variable="isBigger" expression="True"/>
                    </then>
                    <else>
                        <if expression="ToCode(Char(str1, i)) &lt; ToCode(Char(str2, i))">
                            <then>
                                <assign variable="found" expression="True"/>
                                <assign variable="isBigger" expression="False"/>
                            </then>
                            <else/>
                        </if>
                    </else>
                </if>
                <assign variable="i" expression="i + 1"/>
            </while>
        </body>
    </function>
    <function name="updateMap" type="None" variable="">
        <parameters>
            <parameter name="Map" type="String" array="True"/>
            <parameter name="PlayerXY" type="Integer" array="True"/>
            <parameter name="previousXY" type="Integer" array="True"/>
        </parameters>
        <body>
            <declare name="newRow" type="String" array="False" size=""/>
            <declare name="j" type="Integer" array="False" size=""/>
            <assign variable="newRow" expression="&quot;&quot;"/>
            <if expression="PlayerXY[0] &gt;= 0 &amp;&amp; PlayerXY[0] &lt; 29">
                <then>
                    <for variable="j" start="0" end="len(Map[PlayerXY[0]])-1" direction="inc" step="1">
                        <if expression="PlayerXY[1] == j">
                            <then>
                                <assign variable="newRow" expression="newRow &amp; &quot;&amp;&quot;"/>
                            </then>
                            <else>
                                <if expression="char(Map[PlayerXY[0]], j) == &quot;&amp;&quot;">
                                    <then>
                                        <assign variable="newRow" expression="newRow &amp; &quot; &quot;"/>
                                    </then>
                                    <else>
                                        <assign variable="newRow" expression="newRow &amp; char(Map[PlayerXY[0]], j)"/>
                                    </else>
                                </if>
                            </else>
                        </if>
                    </for>
                    <assign variable="Map[PlayerXY[0]]" expression="newRow"/>
                    <assign variable="newRow" expression="&quot;&quot;"/>
                    <if expression="previousXY[0] != PlayerXY[0]">
                        <then>
                            <for variable="j" start="0" end="len(Map[previousXY[0]])-1" direction="inc" step="1">
                                <if expression="char(Map[previousXY[0]], j) == &quot;&amp;&quot;">
                                    <then>
                                        <assign variable="newRow" expression="newRow &amp; &quot; &quot;"/>
                                    </then>
                                    <else>
                                        <assign variable="newRow" expression="newRow &amp; char(Map[previousXY[0]], j)"/>
                                    </else>
                                </if>
                            </for>
                            <assign variable="Map[previousXY[0]]" expression="newRow"/>
                        </then>
                        <else/>
                    </if>
                </then>
                <else/>
            </if>
        </body>
    </function>
    <function name="updateUserScore" type="None" variable="">
        <parameters>
            <parameter name="userName" type="String" array="False"/>
            <parameter name="score" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="numberOfPlayers" type="Integer" array="False" size=""/>
            <assign variable="numberOfPlayers" expression="getLengthOfFile(&quot;scoreboard.txt&quot;)"/>
            <assign variable="numberOfPlayers" expression="numberOfPlayers + 1"/>
            <declare name="usernames" type="String" array="True" size="numberOfPlayers"/>
            <declare name="scores" type="Integer" array="True" size="numberOfPlayers"/>
            <call expression="getUsernamesAndScores(usernames, scores)"/>
            <assign variable="usernames[numberOfPlayers-1]" expression="&quot;&quot;"/>
            <assign variable="scores[numberOfPlayers-1]" expression="0"/>
            <declare name="sizeOfNewArray, i" type="Integer" array="False" size=""/>
            <if expression="wordsInArrayLinearSearch(userName, usernames)">
                <then>
                    <assign variable="sizeOfNewArray" expression="numberOfPlayers - 1"/>
                </then>
                <else>
                    <assign variable="sizeOfNewArray" expression="numberOfPlayers"/>
                </else>
            </if>
            <open expression="&quot;scoreboard.txt&quot;" mode="write"/>
            <for variable="i" start="0" end="sizeOfNewArray-1" direction="inc" step="1">
                <if expression="usernames[i] == &quot;&quot;">
                    <then>
                        <write expression="userName &amp; &quot;,&quot; &amp; score"/>
                    </then>
                    <else>
                        <if expression="userName == usernames[i]">
                            <then>
                                <write expression="userName &amp; &quot;,&quot; &amp; score"/>
                            </then>
                            <else>
                                <write expression="usernames[i] &amp; &quot;,&quot; &amp; scores[i]"/>
                            </else>
                        </if>
                    </else>
                </if>
            </for>
            <close/>
        </body>
    </function>
    <function name="userNameInputIsValid" type="Boolean" variable="isValid">
        <parameters>
            <parameter name="userName" type="String" array="False"/>
        </parameters>
        <body>
            <declare name="isValid" type="Boolean" array="False" size=""/>
            <assign variable="isValid" expression="True"/>
            <if expression="userName == &quot;&quot;">
                <then>
                    <assign variable="isValid" expression="False"/>
                    <output expression="&quot;You have to enter a username.&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="len(userName) &gt; 20">
                        <then>
                            <assign variable="isValid" expression="False"/>
                            <output expression="&quot;Username length cannot exceed 20 characters. Sorry.&quot;" newline="True"/>
                        </then>
                        <else>
                            <declare name="i" type="Integer" array="False" size=""/>
                            <for variable="i" start="0" end="len(userName)-1" direction="inc" step="1">
                                <if expression="(char(userName, i) &lt; &quot;A&quot; || char(userName, i) &gt; &quot;Z&quot;) &amp;&amp; (char(userName, i) &lt; &quot;a&quot; || char(userName, i) &gt; &quot;z&quot;)">
                                    <then>
                                        <assign variable="isValid" expression="False"/>
                                    </then>
                                    <else/>
                                </if>
                            </for>
                            <if expression="!isValid">
                                <then>
                                    <output expression="&quot;You can only use lowercase or uppercase letters. Sorry.&quot;" newline="True"/>
                                </then>
                                <else/>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
        </body>
    </function>
    <function name="wordsInArrayBinarySearch" type="Boolean" variable="wordHasBeenFound">
        <parameters>
            <parameter name="guess" type="String" array="False"/>
            <parameter name="words" type="String" array="True"/>
        </parameters>
        <body>
            <declare name="wordHasBeenFound" type="Boolean" array="False" size=""/>
            <assign variable="wordHasBeenFound" expression="false"/>
            <declare name="left, middle, right" type="Integer" array="False" size=""/>
            <assign variable="left" expression="0"/>
            <assign variable="right" expression="size(words) - 1"/>
            <while expression="left &lt;= right &amp;&amp; &#13;&#10;!wordHasBeenFound">
                <assign variable="middle" expression="(left + right) / 2"/>
                <if expression="guess == words[middle]">
                    <then>
                        <assign variable="wordHasBeenFound" expression="true"/>
                    </then>
                    <else>
                        <if expression="stringOneIsAlphabeticallyBiggerThanStringTwo(guess, words[middle])">
                            <then>
                                <assign variable="left" expression="middle + 1"/>
                            </then>
                            <else>
                                <assign variable="right" expression="middle - 1"/>
                            </else>
                        </if>
                    </else>
                </if>
            </while>
        </body>
    </function>
    <function name="wordsInArrayLinearSearch" type="Boolean" variable="isInArray">
        <parameters>
            <parameter name="guess" type="String" array="False"/>
            <parameter name="words" type="String" array="True"/>
        </parameters>
        <body>
            <declare name="isInArray" type="Boolean" array="False" size=""/>
            <assign variable="isInArray" expression="false"/>
            <declare name="i" type="Integer" array="False" size=""/>
            <for variable="i" start="0" end="size(words)-1" direction="inc" step="1">
                <if expression="guess == words[i]">
                    <then>
                        <assign variable="isInArray" expression="true"/>
                    </then>
                    <else/>
                </if>
            </for>
        </body>
    </function>
