    </function>
    <function name="levelTicTacToe" type="Integer" variable="score">
        <parameters>
            <parameter name="score" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="LoadingScreen" type="String" array="True" size="29"/>
            <call expression="readFileToArray(&quot;TicTacToe_Loading_Screen.txt&quot;, LoadingScreen)"/>
            <call expression="printArrayToDisplay(LoadingScreen)"/>
            <declare name="shouldProceed" type="String" array="False" size=""/>
            <output expression="&quot;Type 'yes' if you wish to proceed to the next level. Be careful not to lose everything.&quot;" newline="True"/>
            <input variable="shouldProceed"/>
            <if expression="makeWordLowercase(shouldProceed) == &quot;yes&quot;">
                <then>
                    <output expression="&quot;Enter a number from 1 to 9 to put an X in that box. 1 being top left, 9 being bottom right&quot;" newline="True"/>
                    <declare name="Board" type="String" array="True" size="3"/>
                    <declare name="i" type="Integer" array="False" size=""/>
                    <for variable="i" start="0" end="2" direction="inc" step="1">
                        <assign variable="Board[i]" expression="&quot;   &quot;"/>
                    </for>
                    <call expression="drawTicTacToeBoard(Board)"/>
                    <declare name="gameOutcome" type="Integer" array="False" size=""/>
                    <assign variable="gameOutcome" expression="0"/>
                    <while expression="gameOutcome == 0">
                        <call expression="playerMakesMove(Board)"/>
                        <assign variable="gameOutcome" expression="gameOver(Board)"/>
                        <call expression="drawTicTacToeBoard(Board)"/>
                        <if expression="gameOutcome == 0">
                            <then>
                                <call expression="robotMakesMove(Board)"/>
                                <assign variable="gameOutcome" expression="gameOver(Board)"/>
                                <call expression="drawTicTacToeBoard(Board)"/>
                            </then>
                            <else/>
                        </if>
                    </while>
                    <if expression="gameOutcome == 1">
                        <then>
                            <assign variable="score" expression="score + 1000"/>
                            <output expression="&quot;Victory!&quot;" newline="True"/>
                            <call expression="sleep(0.4)"/>
                            <call expression="playEnding(&quot;Victory&quot;)"/>
                            <call expression="playIntroOutro(&quot;Outro&quot;)"/>
                        </then>
                        <else>
                            <if expression="gameOutcome == 2">
                                <then>
                                    <output expression="&quot;Tie!&quot;" newline="True"/>
                                    <call expression="sleep(0.4)"/>
                                    <assign variable="score" expression="levelTicTacToe(score)"/>
                                </then>
                                <else>
                                    <output expression="&quot;Defeat!&quot;" newline="True"/>
                                    <call expression="sleep(0.4)"/>
                                    <assign variable="score" expression="0"/>
                                    <call expression="playEnding(&quot;Defeat&quot;)"/>
                                </else>
                            </if>
                        </else>
                    </if>
                </then>
                <else/>
            </if>
        </body>
    </function>
    <function name="levelWordsearch" type="Integer" variable="score">
        <parameters>
            <parameter name="score" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="LoadingScreen" type="String" array="True" size="29"/>
            <call expression="readFileToArray(&quot;Wordsearch_Loading_Screen.txt&quot;, LoadingScreen)"/>
            <call expression="printArrayToDisplay(LoadingScreen)"/>
            <declare name="shouldProceed" type="String" array="False" size=""/>
            <output expression="&quot;Type 'yes' if you wish to proceed to the next level. Be careful not to lose everything.&quot;" newline="True"/>
            <input variable="shouldProceed"/>
            <if expression="(makeWordLowercase(shouldProceed) == &quot;yes&quot;)">
                <then>
                    <output expression="&quot;There are 20 hidden words related to dungeons, puzzles, and mysteries. Your task is to type the correct words into the console.&quot;" newline="True"/>
                    <declare name="wordsearchDisplay" type="String" array="True" size="26"/>
                    <call expression="readFileToArray(&quot;Wordsearch.txt&quot;, wordsearchDisplay)"/>
                    <call expression="printArrayToDisplay(wordsearchDisplay)"/>
                    <declare name="answers, foundWords" type="String" array="True" size="20"/>
                    <call expression="readFileToArray(&quot;Wordserach_Answers.txt&quot;, answers)"/>
                    <declare name="i" type="Integer" array="False" size=""/>
                    <for variable="i" start="0" end="size(foundWords)-1" direction="inc" step="1">
                        <assign variable="foundWords[i]" expression="&quot;&quot;"/>
                    </for>
                    <declare name="numberOfWordsLeft" type="Integer" array="False" size=""/>
                    <assign variable="numberOfWordsLeft" expression="size(answers)"/>
                    <declare name="guess" type="String" array="False" size=""/>
                    <assign variable="guess" expression="&quot;&quot;"/>
                    <while expression="numberOfWordsLeft &gt; 0 &amp;&amp; guess != &quot;HugsW&quot; &amp;&amp; guess != &quot;QUIT&quot;">
                        <output expression="&quot;Enter your guess&quot;" newline="True"/>
                        <input variable="guess"/>
                        <if expression="wordsInArrayBinarySearch(makeWordLowercase(guess), answers)">
                            <then>
                                <if expression="!wordsInArrayLinearSearch(makeWordLowercase(guess), foundWords)">
                                    <then>
                                        <assign variable="numberOfWordsLeft" expression="numberOfWOrdsLeft - 1"/>
                                        <assign variable="foundWords[numberOfWordsLeft]" expression="makeWordLowercase(guess)"/>
                                        <if expression="numberOfWOrdsLeft &gt; 0">
                                            <then>
                                                <output expression="&quot;Congratulations, you've found a word!&quot; &amp; ToChar(10) &amp;&#13;&#10;numberOfWordsLeft &amp; &quot; Left!&quot;" newline="True"/>
                                            </then>
                                            <else/>
                                        </if>
                                    </then>
                                    <else>
                                        <output expression="&quot;Word has already been found!&quot;" newline="True"/>
                                    </else>
                                </if>
                            </then>
                            <else>
                                <if expression="guess != &quot;Finding money in unexpected places&quot;">
                                    <then>
                                        <output expression="&quot;This is not a word we're looking for!&quot;" newline="True"/>
                                    </then>
                                    <else/>
                                </if>
                            </else>
                        </if>
                    </while>
                    <if expression="numberOfWordsLeft == 0">
                        <then>
                            <output expression="&quot;Congratulations, you've found all of the words!!&quot;" newline="True"/>
                        </then>
                        <else/>
                    </if>
                    <if expression="guess != &quot;QUIT&quot;">
                        <then>
                            <call expression="playEnding(&quot;Level Passed&quot;)"/>
                            <assign variable="score" expression="levelTicTacToe(score + 1000)"/>
                        </then>
                        <else>
                            <assign variable="score" expression="0"/>
                            <call expression="playEnding(&quot;Defeat&quot;)"/>
                        </else>
                    </if>
                </then>
                <else/>
            </if>
        </body>
    </function>
    <function name="makeMove" type="None" variable="Position">
        <parameters>
            <parameter name="Map" type="String" array="True"/>
            <parameter name="PlayerXY" type="Integer" array="True"/>
            <parameter name="move" type="String" array="False"/>
        </parameters>
        <body>
            <declare name="previousXY" type="Integer" array="True" size="2"/>
            <assign variable="previousXY[0]" expression="playerXY[0]"/>
            <assign variable="previousXY[1]" expression="playerXY[1]"/>
            <if expression="move == &quot;W&quot; || move == &quot;w&quot;">
                <then>
                    <assign variable="PlayerXY[0]" expression="PlayerXY[0] - 1"/>
                    <if expression="!isMoveValid(Map, PlayerXY)">
                        <then>
                            <assign variable="PlayerXY[0]" expression="PlayerXY[0] + 1"/>
                        </then>
                        <else/>
                    </if>
                </then>
                <else>
                    <if expression="move == &quot;S&quot; || move == &quot;s&quot;">
                        <then>
                            <assign variable="PlayerXY[0]" expression="PlayerXY[0] + 1"/>
                            <if expression="!isMoveValid(Map, PlayerXY)">
                                <then>
                                    <assign variable="PlayerXY[0]" expression="PlayerXY[0] - 1"/>
                                </then>
                                <else/>
                            </if>
                        </then>
                        <else>
                            <if expression="move == &quot;A&quot; || move == &quot;a&quot;">
                                <then>
                                    <assign variable="PlayerXY[1]" expression="PlayerXY[1] - 1"/>
                                    <if expression="!isMoveValid(Map, PlayerXY)">
                                        <then>
                                            <assign variable="PlayerXY[1]" expression="PlayerXY[1] + 1"/>
                                        </then>
                                        <else/>
                                    </if>
                                </then>
                                <else>
                                    <if expression="move == &quot;D&quot; || move == &quot;d&quot;">
                                        <then>
                                            <assign variable="PlayerXY[1]" expression="PlayerXY[1] + 1"/>
                                            <if expression="!isMoveValid(Map, PlayerXY)">
                                                <then>
                                                    <assign variable="PlayerXY[1]" expression="PlayerXY[1] - 1"/>
                                                </then>
                                                <else/>
                                            </if>
                                        </then>
                                        <else/>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
            <call expression="updateMap(Map, PlayerXY, previousXY)"/>
        </body>
    </function>
    <function name="makeWordLowercase" type="String" variable="lowercaseWord">
        <parameters>
            <parameter name="word" type="String" array="False"/>
        </parameters>
        <body>
            <declare name="i" type="Integer" array="False" size=""/>
            <declare name="lowercaseWord" type="String" array="False" size=""/>
            <assign variable="lowercaseWord" expression="&quot;&quot;"/>
            <for variable="i" start="0" end="len(word)-1" direction="inc" step="1">
                <if expression="char(word, i) &gt;= &quot;A&quot; &amp;&amp; char(word, i) &lt;= &quot;Z&quot;">
                    <then>
                        <assign variable="lowercaseWord" expression="lowercaseWord &amp; toChar(ToCode(&quot;a&quot;) + (ToCode(char(word, i)) - ToCode(&quot;A&quot;)))"/>
                    </then>
                    <else>
                        <assign variable="lowercaseWord" expression="lowercaseWord &amp; char(word, i)"/>
                    </else>
                </if>
            </for>
        </body>
    </function>
    <function name="merge" type="None" variable="">
        <parameters>
            <parameter name="names" type="String" array="True"/>
            <parameter name="scores" type="Integer" array="True"/>
            <parameter name="left" type="Integer" array="False"/>
            <parameter name="middle" type="Integer" array="False"/>
            <parameter name="right" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="numberOfElementsLeft, numberOfElementsRight, leftIndex, rightIndex, i" type="Integer" array="False" size=""/>
            <assign variable="numberOfElementsLeft" expression="middle - left + 1"/>
            <assign variable="numberOfElementsRight" expression="right - middle"/>
            <declare name="mergedArrayScores" type="Integer" array="True" size="numberOfElementsLeft + numberOfElementsRight"/>
            <declare name="mergedArrayNames" type="String" array="True" size="numberOfElementsLeft + numberOfElementsRight"/>
            <assign variable="leftIndex" expression="left"/>
            <assign variable="rightIndex" expression="middle + 1"/>
            <for variable="i" start="0" end="(numberOfElementsLeft +&#13;&#10;numberOfElementsRight) - 1" direction="inc" step="1">
                <if expression="leftIndex &lt;= middle &amp;&amp; rightIndex &lt;= right">
                    <then>
                        <if expression="scores[leftIndex] &gt; scores[rightIndex]">
                            <then>
                                <assign variable="mergedArrayNames[i]" expression="names[leftIndex]"/>
                                <assign variable="mergedArrayScores[i]" expression="scores[leftIndex]"/>
                                <assign variable="leftIndex" expression="leftIndex + 1"/>
                            </then>
                            <else>
                                <assign variable="mergedArrayNames[i]" expression="names[rightIndex]"/>
                                <assign variable="mergedArrayScores[i]" expression="scores[rightIndex]"/>
                                <assign variable="rightIndex" expression="rightIndex + 1"/>
                            </else>
                        </if>
                    </then>
                    <else>
                        <if expression="leftIndex &lt;= middle">
                            <then>
                                <assign variable="mergedArrayNames[i]" expression="names[leftIndex]"/>
                                <assign variable="mergedArrayScores[i]" expression="scores[leftIndex]"/>
                                <assign variable="leftIndex" expression="leftIndex + 1"/>
                            </then>
                            <else>
                                <if expression="rightIndex &lt;= right">
                                    <then>
                                        <assign variable="mergedArrayNames[i]" expression="names[rightIndex]"/>
                                        <assign variable="mergedArrayScores[i]" expression="scores[rightIndex]"/>
                                        <assign variable="rightIndex" expression="rightIndex + 1"/>
                                    </then>
                                    <else/>
                                </if>
                            </else>
                        </if>
                    </else>
                </if>
            </for>
            <for variable="i" start="0" end="(numberOfElementsLeft +&#13;&#10;numberOfElementsRight) - 1" direction="inc" step="1">
                <assign variable="names[i + left]" expression="mergedArrayNames[i]"/>
                <assign variable="scores[i + left]" expression="mergedArrayScores[i]"/>
            </for>
        </body>
    </function>
    <function name="mergeSort" type="None" variable="">
        <parameters>
            <parameter name="usernames" type="String" array="True"/>
            <parameter name="scores" type="Integer" array="True"/>
            <parameter name="left" type="Integer" array="False"/>
            <parameter name="right" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="middle" type="Integer" array="False" size=""/>
            <if expression="left &lt; right">
                <then>
                    <assign variable="middle" expression="(left + right) / 2"/>
                    <call expression="mergeSort(usernames, scores, left, middle)"/>
                    <call expression="mergeSort(usernames, scores, middle + 1, right)"/>
                    <call expression="merge(usernames, scores, left, middle, right)"/>
                </then>
                <else/>
            </if>
        </body>
    </function>
    <function name="minimax" type="Integer" variable="bestScore">
        <parameters>
            <parameter name="board" type="String" array="True"/>
            <parameter name="isMaximizing" type="Boolean" array="False"/>
        </parameters>
        <body>
            <declare name="bestScore, score, move" type="Integer" array="False" size=""/>
            <assign variable="score" expression="gameOver(board)"/>
            <if expression="score != 0">
                <then>
                    <assign variable="bestScore" expression="score"/>
                </then>
                <else>
                    <if expression="isMaximizing">
                        <then>
                            <assign variable="bestScore" expression="0"/>
                            <for variable="move" start="0" end="8" direction="inc" step="1">
                                <if expression="isTTTMoveValid(move, board)">
                                    <then>
                                        <call expression="setChar(board, move, &quot;O&quot;)"/>
                                        <assign variable="score" expression="minimax(board, False)"/>
                                        <call expression="setChar(board, move, &quot; &quot;)"/>
                                        <if expression="score &gt; bestScore">
                                            <then>
                                                <assign variable="bestScore" expression="score"/>
                                            </then>
                                            <else/>
                                        </if>
                                    </then>
                                    <else/>
                                </if>
                            </for>
                        </then>
                        <else>
                            <assign variable="bestScore" expression="3"/>
                            <for variable="move" start="0" end="8" direction="inc" step="1">
                                <if expression="isTTTMoveValid(move, board)">
                                    <then>
                                        <call expression="setChar(board, move, &quot;X&quot;)"/>
                                        <assign variable="score" expression="minimax(board, True)"/>
                                        <call expression="setChar(board, move, &quot; &quot;)"/>
                                        <if expression="score &lt; bestScore">
                                            <then>
                                                <assign variable="bestScore" expression="score"/>
                                            </then>
                                            <else/>
                                        </if>
                                    </then>
                                    <else/>
                                </if>
                            </for>
                        </else>
                    </if>
                </else>
            </if>
        </body>
    </function>
    <function name="playEnding" type="None" variable="">
        <parameters>
            <parameter name="ending" type="String" array="False"/>
        </parameters>
        <body>
            <declare name="CongratulationsScreen" type="String" array="True" size="29"/>
            <declare name="fileToReadFrom" type="String" array="False" size=""/>
            <declare name="sleepIntervalUnit" type="Integer" array="False" size=""/>
            <if expression="ending == &quot;Victory&quot;">
                <then>
                    <assign variable="sleepIntervalUnit" expression="1.5"/>
                    <assign variable="fileToReadFrom" expression="&quot;Victory.txt&quot;"/>
                </then>
                <else>
                    <if expression="ending == &quot;Defeat&quot;">
                        <then>
                            <assign variable="sleepIntervalUnit" expression="1.5"/>
                            <assign variable="fileToReadFrom" expression="&quot;Defeat.txt&quot;"/>
                        </then>
                        <else>
                            <if expression="ending == &quot;Level Passed&quot;">
                                <then>
                                    <assign variable="sleepIntervalUnit" expression="1"/>
                                    <assign variable="fileToReadFrom" expression="&quot;Level_Passed.txt&quot;"/>
                                </then>
                                <else>
                                    <if expression="ending == &quot;Farewell&quot;">
                                        <then>
                                            <assign variable="sleepIntervalUnit" expression="1"/>
                                            <assign variable="fileToReadFrom" expression="&quot;Farewell.txt&quot;"/>
                                        </then>
                                        <else>
                                            <assign variable="sleepIntervalUnit" expression="0"/>
                                            <assign variable="fileToReadFrom" expression="&quot;Quit_Empty_Screen.txt&quot;"/>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
            <call expression="readFileToArray(fileToReadFrom, CongratulationsScreen)"/>
            <call expression="printArrayToDisplay(CongratulationsScreen)"/>
            <call expression="sleep(sleepIntervalUnit)"/>
        </body>
